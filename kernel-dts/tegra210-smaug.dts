#include "tegra210-smaug-common.dtsi"
#include "tegra210-smaug-common-disp.dtsi"
#include "smaug-platforms/tegra210-smaug-power-tree.dtsi"
#include "smaug-platforms/tegra210-smaug-emc.dtsi"
#include "smaug-platforms/tegra210-smaug-pinmux.dtsi"
#include "smaug-platforms/tegra210-smaug-keys.dtsi"
#include "smaug-platforms/tegra210-smaug-thermal-nct72.dtsi"

/ {

	model = "Google Tegra210 Smaug Rev All";
	compatible = "google,smaug-rev8", "google,smaug-rev7",
		     "google,smaug-rev6", "google,smaug-rev5",
		     "google,smaug-rev4", "google,smaug-rev3",
		     "google,smaug-rev2", "google,smaug-rev1",
		     "google,smaug", "nvidia,tegra210";

	nvidia,dtsfilename = __FILE__;
	nvidia,boardids = "2595:0000:A0";
	nvidia,proc-boardid = "2595:0000:A0";
	nvidia,pmu-boardid = "2595:0000:A0";

	#address-cells = <2>;
	#size-cells = <2>;

	serial-number = "1234";

	firmware {
		android {
			compatible = "android,firmware";
			system_root_blkdev = "/dev/mmcblk0p4";
			fstab {
				compatible = "android,fstab";
				vendor {
					compatible = "android,vendor";
					dev = "/dev/block/platform/sdhci-tegra.3/by-name/VNR";
					type = "ext4";
					mnt_flags = "ro,barrier=1,discard";
					fsmgr_flags = "wait";
					status = "ok";
				};
			};
		};
	};

	chosen {
		bootargs = "cros_legacy init=/init root=/dev/mmcblk0p4 rootfstype=ext4 rw rootwait skip_initramfs vmalloc=256M tegra_fbmem=0x800000@0x969ee000 nvdec_enabled=0 vpr_resize console=ttyS0,115200n8 earlycon=uart8250,mmio32,0x70006000 lp0_vec=2672@0xf9aff000 tsec=32M@4064M video=tegrafb no_console_suspend=1 otf_key=914e3cd033c06c6370b630bb6ec9dd4e androidboot.selinux=permissive firmware_class.path=/vendor/firmware";
		nvidia,bluetooth-mac = "11:22:33:44:55:66";
		nvidia,wifi-mac = "00:90:4c:11:22:33";
	};

	gpu {
		status = "okay";
	};

	tlk {
		status = "okay";
	};

	cpu_edp {
		status = "okay";
	};

	gpu_edp {
		status = "okay";
	};

	cpus {
		idle-states {
			entry-method = "psci";

			c7 {
				arm,psci-suspend-param = <0x00010007>;
			};

			/delete-node/ cc6;
		};
	};

	psci {
		nvidia,system-lp0-disable;
	};

	spi@7000d400 {
		status = "okay";
/*		spi@0 {
			compatible = "spidev";
			reg = <0>;
			spi-max-frequency = <13000000>;
		};*/
	};
	spi@7000da00 {
		status = "okay";
/*		spi@0 {
			compatible = "spidev";
			reg = <0>;
			spi-max-frequency = <13000000>;
		};*/
	};

	i2c1: i2c@7000c000 {
		status = "okay";
		clock-frequency = <1000000>;

		touchscreen: i2c-hid-dev@20 {
				compatible = "hid-over-i2c";
				reg = <0x20>;
				status = "okay";
				hid-descr-addr = <0x0020>;
				interrupt-parent = <&gpio>;
				interrupts = <TEGRA_GPIO(X, 1) IRQ_TYPE_EDGE_FALLING>;
		};
	};

	i2c2: i2c@7000c400 {
		status = "okay";
		clock-frequency = <1000000>;

		ec@1e {
			compatible = "google,cros-ec-i2c";
			reg = <0x1e>;
			interrupt-parent = <&gpio>;
			interrupts = <TEGRA_GPIO(Z, 1) IRQ_TYPE_LEVEL_LOW>;
			wakeup-source;

			ec_i2c_0: i2c-tunnel {
				compatible = "google,cros-ec-i2c-tunnel";
				#address-cells = <1>;
				#size-cells = <0>;

				google,remote-bus = <0>;

				battery: bq27742@55 {
					compatible = "ti,bq27742";
					reg = <0x55>;
					battery-name = "battery";
				};
			};

			usbc_extcon: extcon {
				compatible = "google,extcon-usbc-cros-ec";
				google,usb-port-id = <0>;

				#extcon-cells = <0>;
			};
		};
	};

	i2c3: i2c@7000c500 {
		status = "okay";
		clock-frequency = <1000000>;
		tpm@20 {
			compatible = "infineon,slb9645tt";
			reg = <0x20>;
			powered-while-suspended;
			status = "okay";
		};
	};

	i2c6: i2c@7000d100 {
		status = "okay";
		clock-frequency = <1000000>;
		nvidia,clock-always-on;

		backlight: lp8557-backlight@2c {
				status = "okay";
				compatible = "ti,lp8557";
				reg = <0x2c>;
				power-supply = <&pplcd_vdd>;
				enable-supply = <&pp1800_lcdio>;
				bl-name = "lp8557-backlight";
				dev-ctrl = /bits/ 8 <0x01>;
				init-brt = /bits/ 8 <0xFF>;
				/* Full scale current, 20mA */
				rom_11h {
						rom-addr = /bits/ 8 <0x11>;
						rom-val = /bits/ 8 <0x05>;
				};
				/* Frequency = 4.9kHz, magic undocumented val */
				rom_12h {
						rom-addr = /bits/ 8 <0x12>;
						rom-val = /bits/ 8 <0x29>;
				};
				/* Boost freq = 1MHz, BComp option = 1 */
				rom_13h {
						rom-addr = /bits/ 8 <0x13>;
						rom-val = /bits/ 8 <0x03>;
				};
				/* 4V OV, 6 output LED string enabled */
				rom_14h {
						rom-addr = /bits/ 8 <0x14>;
						rom-val = /bits/ 8 <0xbf>;
				};
		};
	};

	pwm@7000a000 {
		status = "disabled";
	};

};
